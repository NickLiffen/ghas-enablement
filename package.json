{
  "name": "github-helper",
  "version": "1.0.0",
  "description": "GitHub Helpers - GHAS Code Scanning File",
  "main": "lib/src/index.js",
  "scripts": {
    "build": "npx tsc",
    "prepare": "husky install",
    "prettier:write": "npx prettier --write '**/*.{ts,json,md,yaml,yml}'",
    "prettier:check": "npx prettier --check '**/*.{ts,json,md,yaml,yml}'",
    "lint:check": "npx eslint '**/*.{ts,json}'",
    "lint:write": "npx eslint --fix '**/*.{ts,json}'",
    "test": "npx jest --coverage --verbose",
    "getRepos": "node ./lib/getRepos.js",
    "getOrgs": "node ./lib/getOrgs.js",
    "enableOrg": "node ./lib/enableOrg.js",
    "start": "npm run build && node ./lib/enable.js"
  },
  "keywords": [
    "GitHub",
    "Advanced Security"
  ],
  "devDependencies": {
    "@octokit/plugin-retry": "^6.0.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/aws-lambda": "^8.10.125",
    "@types/debug": "^4.1.10",
    "@types/jest": "^29.5.7",
    "@types/json-schema": "^7.0.14",
    "@types/mock-fs": "^4.13.3",
    "@types/node": "^20.8.10",
    "@types/randomstring": "^1.1.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "mock-fs": "^5.2.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "author": "Nick Liffen",
  "license": "ISC",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@octokit/auth-app": "^6.0.1",
    "@octokit/core": "^5.0.1",
    "@octokit/plugin-paginate-rest": "^9.1.2",
    "@octokit/plugin-throttling": "^8.1.2",
    "@octokit/request-error": "^5.0.1",
    "@octokit/types": "^12.0.0",
    "debug": "^4.3.4",
    "delay": "^6.0.0",
    "dotenv": "^16.3.1",
    "node": "^21.1.0",
    "randomstring": "^1.3.0"
  }
}
