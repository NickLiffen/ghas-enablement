{
	"name": "github-helper",
	"version": "1.0.0",
	"description": "GitHub Helpers - GHAS Code Scanning File",
	"main": "lib/src/index.js",
	"scripts": {
		"build": "npx tsc",
		"format": "npx prettier --write '**/*.ts'",
		"format-check": "npx prettier --check '**/*.ts'",
		"lint": "npx eslint . --ext .ts",
		"lint-fix": "npx eslint --fix . --ext .ts",
		"test": "npx jest --coverage --verbose",
		"getReposByUser": "node ./lib/src/get-repos-by-user.js",
		"getReposByLanguage": "node ./lib/src/get-repo-by-language.js",
		"start": "node ./lib/src/enable.js"
	},
	"keywords": [
		"GitHub",
		"Advanced Security"
	],
	"devDependencies": {
		"@tsconfig/node14": "^1.0.1",
		"@types/aws-lambda": "^8.10.81",
		"@types/debug": "^4.1.7",
		"@types/jest": "^26.0.24",
		"@types/json-schema": "^7.0.8",
		"@types/mock-fs": "^4.13.1",
		"@types/node": "^16.4.3",
		"@types/randomstring": "^1.1.7",
		"@typescript-eslint/eslint-plugin": "^4.28.5",
		"@typescript-eslint/parser": "^4.28.5",
		"eslint": "^7.31.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-jest": "^24.4.0",
		"eslint-plugin-prettier": "^3.4.0",
		"jest": "^27.0.6",
		"mock-fs": "^5.0.0",
		"prettier": "^2.3.2",
		"ts-jest": "^27.0.4",
		"ts-node": "^10.1.0",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5"
	},
	"author": "Nick Liffen",
	"license": "ISC",
	"engines": {
		"node": "14"
	},
	"dependencies": {
		"@octokit/core": "^3.5.1",
		"@octokit/plugin-paginate-rest": "^2.14.0",
		"@octokit/plugin-retry": "^3.0.9",
		"@octokit/plugin-throttling": "^3.5.1",
		"@octokit/request-error": "^2.1.0",
		"@octokit/types": "^6.21.1",
		"debug": "^4.3.2",
		"delay": "^5.0.0",
		"dotenv": "^10.0.0",
		"randomstring": "^1.2.1"
	}
}
