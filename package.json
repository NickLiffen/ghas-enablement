{
  "name": "github-helper",
  "version": "1.0.0",
  "description": "GitHub Helpers - GHAS Code Scanning File",
  "main": "lib/src/index.js",
  "scripts": {
    "build": "npx tsc",
    "prepare": "husky install",
    "prettier:write": "npx prettier --write '**/*.{ts,json,md,yaml,yml}'",
    "prettier:check": "npx prettier --check '**/*.{ts,json,md,yaml,yml}'",
    "lint:check": "npx eslint '**/*.{ts,json}'",
    "lint:write": "npx eslint --fix '**/*.{ts,json}'",
    "test": "npx jest --coverage --verbose",
    "getRepos": "npm run build && node ./lib/getRepos.js",
    "getOrgs": "npm run build && node ./lib/getOrgs.js",
    "start": "npm run build && node ./lib/enable.js"
  },
  "keywords": [
    "GitHub",
    "Advanced Security"
  ],
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/aws-lambda": "^8.10.95",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.1",
    "@types/json-schema": "^7.0.11",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^17.0.30",
    "@types/randomstring": "^1.1.8",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^28.0.3",
    "mock-fs": "^5.1.2",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "author": "Nick Liffen",
  "license": "ISC",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@octokit/auth-app": "^3.6.1",
    "@octokit/core": "^3.6.0",
    "@octokit/plugin-paginate-rest": "^2.17.0",
    "@octokit/plugin-retry": "^3.0.9",
    "@octokit/plugin-throttling": "^3.6.2",
    "@octokit/request-error": "^2.1.0",
    "@octokit/types": "^6.34.0",
    "debug": "^4.3.4",
    "delay": "^5.0.0",
    "dotenv": "^16.0.0",
    "randomstring": "^1.2.2"
  }
}
