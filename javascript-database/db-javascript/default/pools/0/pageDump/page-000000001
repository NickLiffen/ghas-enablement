  query getRepositoriesQuery($slug: String!, $after: String, $first: Int = 100) {
    viewer {
      login
    }
    rateLimit {
      limit
      cost
      remaining
      resetAt
      used
    }
    organization(login: $slug) {
      repositories(first: $first, after: $after, affiliations: ORGANIZATION_MEMBER, isFork: false) {
        nodes {
          nameWithOwner
          isArchived
          viewerPermission
          visibility
          primaryLanguage {
              name
            }
        }
        totalCount
        pageInfo {
          hasNextPage
          endCursor
        }
      }
    }
  }
